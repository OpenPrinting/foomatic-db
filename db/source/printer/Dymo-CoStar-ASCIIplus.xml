<printer id="printer/Dymo-CoStar-ASCIIplus">
  <make>Dymo-CoStar</make>
  <model>ASCII+</model>
  <mechanism><!--not "color"-->
    <resolution>
      <dpi>
        <x>200</x>
        <y>200</y>
      </dpi>
    </resolution>
    <consumables>
      <comments>
        <en>thermal; special labels</en>
      </comments>
    <!--one or more "partno" elements.--></consumables>
  </mechanism><!--no "url"-->
  <lang>
    <proprietary/><!--no pjl-->
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
  <!--no known parport probe information--></autodetect>
  <functionality>A</functionality>
  <driver>pbm2lwxl</driver>
  <unverified/><!--no "contrib_url"-->
  <comments>
    <en>2.25 inch wide label printer; driver by Mark Whitis&lt;br&gt;Should work; untested</en>
  </comments>
</printer>
