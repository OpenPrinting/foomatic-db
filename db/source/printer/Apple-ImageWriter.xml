<printer id="printer/Apple-ImageWriter">
  <make>Apple</make>
  <model>ImageWriter</model>
  <mechanism>
    <dotmatrix/><!--not "color"-->
    <resolution>
      <dpi>
        <x>120</x>
        <y>144</y>
      </dpi>
    </resolution>
    <consumables>
    <!--no comments--><!--one or more "partno" elements.--></consumables>
  </mechanism>
  <url>http://www.info.apple.com/info.apple.com/applespec/applespec.taf?cat=printer&amp;RID=2</url>
  <lang>
    <proprietary/><!--no pjl-->
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
  <!--no known parport probe information--></autodetect>
  <functionality>A</functionality>
  <driver>iwhi</driver><!--verified--><!--no "contrib_url"-->
  <comments>
    <en>Two gs drivers, iwhi for high resolution and iwlo for low.</en>
  </comments>
</printer>
