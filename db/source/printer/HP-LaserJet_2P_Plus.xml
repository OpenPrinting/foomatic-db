<printer id="printer/HP-LaserJet_2P_Plus">
  <make>HP</make>
  <model>LaserJet 2P Plus</model>
  <mechanism>
    <laser/><!--not "color"-->
    <resolution>
      <dpi>
        <x>300</x>
        <y>300</y>
      </dpi>
    </resolution>
    <consumables>
    <!--no comments--><!--one or more "partno" elements.--></consumables>
  </mechanism><!--no "url"-->
  <lang>
    <pcl level="4"/>
    <text>
      <charset>us-ascii</charset>
    </text>
  </lang>
  <autodetect>
  <!--no known parport probe information--></autodetect>
  <functionality>A</functionality>

  <driver>ljet2p</driver><!--verified--><!--no "contrib_url"-->
  <comments>
  <en>
    For basic printing functionality use the &lt;a 
    href=&quot;http://hplipopensource.com/&quot;&gt;HPIJS 
    driver &lt;/a&gt;. For advanced functionality such
