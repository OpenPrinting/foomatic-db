printf &apos;\033%%-12345X@PJL EOJ\n\033%%-12345X&apos;) |&&
sed -e &quot;1,/^\x25\x21.*/ { &&
s/^\x25\x21.*/&%E/;  &&
s/@PJL SET DATE=\x22\x26year\x3B\x2F\x26month\x3B\x2F\x26date\x3B\x22/&&
`date +&apos;@PJL SET DATE=\x2220%y\x2F%m\x2F%d\x22\x0A@PJL SET TIME=\x22%T\x22&apos;`/g; &&
/@PJL SET TIME=\x22\x26hour\x3B\x3A\x26min\x3B\x3A\x26sec\x3B\x22/D;&&
s/\x26user\x3B/unknown/g; &&
s/\x26title\x3B/unknown/g; &&
}&quot;"
*End

