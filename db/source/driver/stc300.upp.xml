<driver id="driver/stc300.upp">
 <name>stc300.upp</name>
 <url>http://www.ghostscript.com/</url>
 <obsolete replace="gutenprint" />
 <execution>
  <uniprint />
  <prototype>gs %B -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE %A%Z -sOutputFile=- -</prototype>
  <ppdentry>
   *DefaultResolution: 360dpi
  </ppdentry>
 </execution>
 
 <printers>
  <printer>
   <id>printer/Epson-Stylus_Color_300</id><!-- Epson Stylus Color 300 -->
  </printer>
  <printer>
   <id>printer/Epson-MJ_520C</id>
  </printer>
 </printers>
</driver>
