<driver id="driver/escpage">
  <name>escpage</name>
  <url>http://www.openprinting.org/download/japanese_printers/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=escpage%A%Z -sOutputFile=- -</prototype>
  </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
  </comments>
  <printers>
    <printer>
      <id>printer/Epson-LP-xx00</id>
    </printer>
  </printers>
</driver>
