<driver id="driver/ppmtocpva">
  <name>ppmtocpva</name>
  <url>http://www.stevens-bradfield.com/ppmtomd/</url>
  <obsolete replace="ppmtomd" />
  <thirdpartysupplied />
  <execution>
    <filter />
    <prototype>gs -q -dNOPAUSE -dBATCH -dPARANOIDSAFER -sDEVICE=ppmraw -sOutputFile=-%A%B%C</prototype>
  </execution>
  <comments>
    <en>This driver works probably also on the Alps MD-XXXX printers.</en>
  </comments>
  <printers>
    <printer>
      <id>printer/Alps-MD-1000</id>
    </printer>
    <printer>
      <id>printer/Alps-MD-1300</id>
    </printer>
    <printer>
      <id>printer/Alps-MD-1500</id>
    </printer>
    <printer>
      <id>printer/Alps-MD-2000</id>
    </printer>
    <printer>
      <id>printer/Alps-MD-4000</id>
    </printer>
    <printer>
      <id>printer/Citizen-printiva600C</id>
    </printer>
  </printers>
</driver>
