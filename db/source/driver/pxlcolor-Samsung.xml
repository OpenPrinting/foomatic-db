<driver id="driver/pxlcolor-Samsung">
  <name>pxlcolor-Samsung</name>
  <url>http://samsung.com</url>
  <supplier>Samsung</supplier>
  <manufacturersupplied>Samsung</manufacturersupplied>
  <license>MIT</license>
  <supportcontacts>
	  <supportcontact url="http://www.samsung.com/printer/" level="voluntary">Samsung Support</supportcontact>
  </supportcontacts>
  <shortdescription>
    <en>
      PPD files for Samsung's PCL-XL color printers, supplied by Samsung
    </en>
  </shortdescription>
  <packages>
    <package scope="general">openprinting-ppds-pxlcolor-samsung</package>
  </packages>
  <functionality>
    <color />
    <text>100</text>
    <lineart>100</lineart>
    <graphics>100</graphics>
    <photo>100</photo>
  </functionality>
  <execution>
    <ghostscript />
  </execution>
  <comments>
    <en>

      This driver is intended for Samsung PCL-XL color printers and
      MFP devices.

    </en>
  </comments>
</driver>
