<driver id="driver/bjc800j">
  <name>bjc800j</name>
  <url>http://www.openprinting.org/download/japanese_printers/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -dNOMEDIAATTRS -dNOINTERPOLATE -sDEVICE=bjc800j%A%Z -sOutputFile=- -</prototype>
  </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
  </comments>
  <printers>
    <printer>
      <id>printer/Canon-BJC-680J</id>
    </printer>
  </printers>
</driver>
