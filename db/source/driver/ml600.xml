<driver id="driver/ml600">
  <name>ml600</name>
  <url>http://www.openprinting.org/download/japanese_printers/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=ml600%A%Z -sOutputFile=- -</prototype>
  </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
  </comments>
  <printers>
    <printer>
      <id>printer/Oki-Microline_600CL</id>
    </printer>
    <printer>
      <id>printer/Oki-Microline_620CL</id>
    </printer>
  </printers>
</driver>
