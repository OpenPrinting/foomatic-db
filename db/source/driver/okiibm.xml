<driver id="driver/okiibm">
 <name>okiibm</name>
 <url>http://www.cs.wisc.edu/~ghost/doc/printer.htm</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=okiibm%A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/Oki-Microline_IBM_compatible_9_pin</id><!-- Oki Microline IBM compatible 9 pin -->
  </printer>
  <printer>
   <id>printer/Oki-ML_320</id>
  </printer>
  <printer>
   <id>printer/Oki-ML_321</id>
  </printer>
 </printers>
</driver>