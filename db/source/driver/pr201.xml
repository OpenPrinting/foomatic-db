<driver id="driver/pr201">
  <name>pr201</name>
  <url>http://www.linuxprinting.org/download/japanese_printers/</url>
  <execution>
    <ghostscript/>
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=pr201%A%Z -sOutputFile=- -</prototype>
    </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
    </comments>
  <printers>
    <printer>
      <id>printer/NEC-PC-PR201</id>
      </printer>
    </printers>
  </driver>
