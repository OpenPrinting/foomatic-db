<driver id="driver/stcany.upp">
 <name>stcany.upp</name>
 <url>http://www.ghostscript.com/doc/gnu/7.05/Devices.htm#Uniprint</url>
 <execution>
  <uniprint />
  <prototype>gs @stcany.upp -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE %A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/Epson-Stylus_Color_300</id><!-- Epson Stylus Color 300 -->
  </printer>
  <printer>
   <id>printer/Epson-MJ_520C</id>
  </printer>
  <printer>
   <id>printer/Epson-Stylus_Color_777</id><!-- Epson Stylus Color 777 -->
  </printer>
 </printers>
</driver>