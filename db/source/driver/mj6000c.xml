<driver id="driver/mj6000c">
  <name>mj6000c</name>
  <url>http://www.linuxprinting.org/download/japanese_printers/</url>
  <execution>
    <ghostscript/>
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=mj6000c%A%Z -sOutputFile=- -</prototype>
    </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
    </comments>
  <printers>
    <printer>
      <id>printer/Epson-Stylus_Color_1520</id>
      </printer>
     <printer>
      <id>printer/Epson-Stylus_Color_400</id>
      </printer>
     <printer>
      <id>printer/Epson-Stylus_Color_800</id>
      </printer>
    </printers>
  </driver>
