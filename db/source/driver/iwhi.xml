<driver id="driver/iwhi">
 <name>iwhi</name>
 <url>http://www.ghostscript.com/</url>
 <execution>
  <ghostscript />
  <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE %B%A%Z -sOutputFile=- -</prototype>
 </execution>
 
 <printers>
  <printer>
   <id>printer/Apple-ImageWriter</id><!-- Apple ImageWriter -->
   <comments>
    <en>High resolution</en>
   </comments>
  </printer>
  <printer>
   <id>printer/Apple-ImageWriter_II</id><!-- Apple ImageWriter II -->
   <comments>
    <en>High resolution</en>
   </comments>
  </printer>
  <printer>
   <id>printer/Apple-ImageWriter_LQ</id><!-- Apple ImageWriter LQ -->
   <comments>
    <en>High resolution</en>
   </comments>
  </printer>
 </printers>
</driver>