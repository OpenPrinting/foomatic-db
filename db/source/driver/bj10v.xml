<driver id="driver/bj10v">
  <name>bj10v</name>
  <url>http://www.openprinting.org/download/japanese_printers/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=bj10v%A%Z -sOutputFile=- -</prototype>
  </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
  </comments>
  <printers>
    <printer>
      <id>printer/Canon-BJ-10v</id>
    </printer>
    <printer>
      <id>printer/Canon-BJ-15v</id>
    </printer>
  </printers>
</driver>
