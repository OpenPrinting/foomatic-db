<driver id="driver/stc500p.upp">
  <name>stc500p.upp</name>
  <url>http://www.ghostscript.com/</url>
  <obsolete replace="gutenprint" />
  <thirdpartysupplied />
  <execution>
    <uniprint />
    <prototype>gs %B -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE %A%Z -sOutputFile=- -</prototype>
  </execution>
  <comments>
    <en>The printing system/execution information for this driver serves for all the
stc500*.upp drivers.</en>
  </comments>
  <printers>
    <printer>
      <id>printer/Epson-Stylus_Color_400</id>
    </printer>
    <printer>
      <id>printer/Epson-Stylus_Color_500</id>
      <comments>
        <en>360dpi</en>
      </comments>
    </printer>
  </printers>
</driver>
