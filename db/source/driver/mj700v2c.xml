<driver id="driver/mj700v2c">
  <name>mj700v2c</name>
  <url>http://www.linuxprinting.org/download/japanese_printers/</url>
  <execution>
    <ghostscript/>
    <prototype>gs -q -dBATCH -dSAFER -dQUIET -dNOPAUSE -sDEVICE=mj700v2c%A%Z -sOutputFile=- -</prototype>
    </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
    </comments>
  <printers>
    <printer>
      <id>printer/Epson-MachJet</id>
      </printer>
     <printer>
      <id>printer/Epson-Stylus</id>
      </printer>
    </printers>
  </driver>
