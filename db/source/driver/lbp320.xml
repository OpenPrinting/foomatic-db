<driver id="driver/lbp320">
  <name>lbp320</name>
  <url>http://www.openprinting.org/download/japanese_printers/</url>
  <thirdpartysupplied />
  <execution>
    <ghostscript />
    <prototype>gs -q -dBATCH -dPARANOIDSAFER -dQUIET -dNOPAUSE -sDEVICE=lbp320%A%Z -sOutputFile=- -</prototype>
  </execution>
  <comments>
    <en>
      Special japanese printer driver.
      </en>
  </comments>
  <printers>
    <printer>
      <id>printer/Canon-LBP-320_Pro</id>
    </printer>
    <printer>
      <id>printer/Canon-LBP-350</id>
    </printer>
  </printers>
</driver>
